{
  "name": "@wmfs/tymly-diaries-plugin",
  "version": "0.0.0-semantically-released",
  "description": "Plugin to handle bookings and appointment kind of things within Tymly framework",
  "homepage": "https://github.com/wmfs/tymly-diaries-plugin#readme",
  "author": "West Midlands Fire Service",
  "keywords": [
    "tymly",
    "plugin",
    "diary",
    "events",
    "appointments"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly-diaries-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/tymly-diaries-plugin/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "boom": "7.2.0",
    "debug": "3.1.0",
    "lodash": "4.17.10",
    "moment": "2.22.2"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.3",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "semantic-release": "^15.5.2",
    "standard": "11.0.1",
    "@wmfs/hl-pg-client": "1.1.3",
    "@wmfs/tymly": "1.4.1",
    "@wmfs/tymly-test-helpers": "1.0.1"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {"type": "build", "scope":"deps", "release": "minor"},
        {"type": "build", "scope":"deps-dev", "release": "patch"}
      ]
    }
  }
}
