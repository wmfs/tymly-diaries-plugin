{
  "Comment": "State machine to get available times based on a date.",
  "version": "1.0",
  "StartAt": "GetAvailableTimes",
  "States": {
    "GetAvailableTimes": {
      "Type": "Task",
      "Resource": "module:getAvailableDiarySlots",
      "ResourceConfig": {
        "diaryId": "doctors"
      },
      "End": true
    }
  },
  "restrictions": [
    {
      "roleId": "$authenticated",
      "allows": [
        "*"
      ]
    }
  ]
}